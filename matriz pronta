/******************************************************************************

matriz pronta, apresentar o resultado

*******************************************************************************/
#include <stdio.h>

int main()
{
    int i;
    int j;
    int matriz [3][3]= {{4,5,6},{7,8,9},{3,2,1}};
 
    for(i=0; i<3; i++)
    {
        for(j=0; j<3; j++)
        {
            printf("%d\t", matriz[i][j]);
        }
        printf("\n");
    }
