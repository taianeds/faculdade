/*****************************************************************************

ler o resultado e apresentar matriz ARRUMAR

*******************************************************************************/
#include <stdio.h>

int main()
{
    int i;
    int j;
    int temp;
    int matriz [3][3]= 0;
    
    for(i=0; i<3; i++)
    {
        for(j=0; j<3; j++)
        {
            printf("digite a linha %d coluna %d:", i+1, j+1);
            scanf("%d", &temp);
        //printf ("\n");
        matriz [i][j] = temp;
        }
        printf("\n");
    }
        return 0;
        
}
